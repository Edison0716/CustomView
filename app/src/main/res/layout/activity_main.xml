<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mac.myapplication.MainActivity">
    <com.example.mac.myapplication.CircleView
        android:visibility="gone"
        android:id="@+id/circleView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></com.example.mac.myapplication.CircleView>
    <com.example.mac.myapplication.RectView
        android:id="@+id/rectView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></com.example.mac.myapplication.RectView>
    <Button
        android:text="发送消息"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="sentRec"/>
</RelativeLayout>
